<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="kr.ac.kpu.se2018158037.tensor_test1">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET"
        android:usesCleartextTraffic="true"/>
    <uses-feature
        android:name="android.hardware.camera"
        android:required="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        >
        <activity android:name=".Camera_activity">
            android:label="CameraActivity"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>

        <activity android:name=".firbase">
            android:label="firbase"
        </activity>

        <activity android:name=".LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.SEARCH" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
            android:label="LoginActivity"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>

        <activity android:name=".PlantInfoActivity">
            android:label="PlantInfoActivity"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>

        <activity android:name=".PlantInfo2">
            android:label="PlantInfo2"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>

        <activity android:name=".album_activity">
            android:label="album_show"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>

        <activity android:name=".RegisterActivity">
            android:label="RegisterActivity"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>

        <activity android:name=".MainActivity">
            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>

        <activity android:name=".FavorActivity">
            android:label="FavorActivity"
            android:theme="@style/Theme.AppCompat.Dialog"
        </activity>
    </application>

</manifest>